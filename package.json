{
  "name": "website-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prettier": "npx prettier --write \"**/*.tsx\"",
    "build": "next build",
    "export": "echo. > docs/.nojekyll",
    "deploy": "npm run prettier && npm run build && npm run export",
    "commit": "git add . && git commit -m \"commit\"",
    "push": "git push -u origin master",
    "upload": "npm run deploy && npm run commit && npm run push"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "next": "14.2.15",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "gh-pages": "^6.2.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
